{
  "compilerOptions": {
    "declaration": false,
    "noImplicitAny": true,
    "noEmitOnError": true,
    "noUnusedLocals": true,
    "noImplicitReturns": true,
    "strictNullChecks": true,
    "allowUnusedLabels": false,
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "ES5",
    "outDir": "./dist",
    "lib": ["dom", "es5", "es2015.collection", "es2015.promise"],
    "types": [],
    "baseUrl": "./node_modules"
  },
  "exclude": ["node_modules"]
}
