{
  "name": "swift-playground",
  "version": "0.0.1",
  "description": "Swift Online Playground with Code IntelliSense",
  "main": "index.js",
  "repository": "https://github.com/satishbabariya/swift-online-playground.git",
  "author": "Satish Babariya",
  "license": "Apache-2.0",
  "engines": {
    "yarn": "1.0.x || >=1.2.1",
    "node": ">=10.2.0"
  },
  "scripts": {
    "start": "webpack-dev-server",
    "clean": "rimraf dist",
    "build": "yarn build:server && yarn build:client",
    "build:client": "webpack",
    "build:tsc": "tsc --p tsconfig-server.json",
    "build:server": "webpack --config=server.webpack.config.js",
    "watch": "tsc --p tsconfig-server.json --watch & webpack --watch",
    "watch:client": "webpack --watch",
    "watch:server": "tsc --p tsconfig-server.json --watch",
    "prebuild": "yarn clean"
  },
  "dependencies": {
    "@phosphor/widgets": "^1.6.0",
    "express": "^4.16.4",
    "monaco-editor": "^0.15.6",
    "monaco-languageclient": "^0.6.3",
    "normalize-url": "^4.3.0",
    "reconnecting-websocket": "^3.2.2",
    "vscode-languageserver": "4.0.0",
    "vscode-ws-jsonrpc": "^0.0.2-2",
    "ws": "^7.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/node": "^11.13.8",
    "@types/ws": "^6.0.1",
    "awesome-typescript-loader": "^5.2.1",
    "copy-webpack-plugin": "^5.0.0",
    "css-loader": "^2.1.0",
    "html-webpack-plugin": "^3.2.0",
    "monaco-editor-webpack-plugin": "^1.7.0",
    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
    "typescript": "^3.3.3333",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  }
}
